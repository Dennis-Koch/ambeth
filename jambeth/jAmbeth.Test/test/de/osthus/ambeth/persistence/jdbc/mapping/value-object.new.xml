<?xml version="1.0" encoding="UTF-8"?>
<value-object-mappings xmlns="http://osthus.de/ambeth/ambeth_vom_2_0">

	<entity class="de.osthus.ambeth.model.Material">
		<value-object class="de.osthus.ambeth.transfer.MaterialVO" />
		<value-object class="de.osthus.ambeth.transfer.MaterialSmallVO" />
	</entity>

	<entity class="de.osthus.ambeth.persistence.jdbc.mapping.models.OneToManyEntity">
		<value-object class="de.osthus.ambeth.persistence.jdbc.mapping.models.OneToManyEntityVO">
			<basic name="MyEmbeddedType.NameString" name-in-entity="MyEmbedded.Name" />
			<ignore name="MyEmbeddedType.ValueNumber" />
			<relation name="ByListType" list-type="true"
				target-value-object="de.osthus.ambeth.persistence.jdbc.mapping.models.OneToManyEntityVO" />
			<relation name="ByREFListType" name-in-entity="ByRefListType" list-type="true" />
			<relation name="SelfReferencingEntities"
				target-value-object="de.osthus.ambeth.persistence.jdbc.mapping.models.SelfReferencingEntityVO" />
		</value-object>
	</entity>

	<entity class="de.osthus.ambeth.persistence.jdbc.mapping.models.SelfReferencingEntity">
		<value-object class="de.osthus.ambeth.persistence.jdbc.mapping.models.SelfReferencingEntityVO">
			<basic name="Values" target-element-type="java.lang.String" />
		</value-object>
	</entity>

</value-object-mappings>
