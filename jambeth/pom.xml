				<artifactId>maven-jxr-plugin</artifactId>
				<version>2.3</version>
			</plugin>
		</plugins>
	</reporting>

	<profiles>
		<profile>
			<id>osthus-local</id>
			<activation>
				<property>
					<name>!deploy.env</name>
				</property>
			</activation>
			<properties>
				<server.id>tomcat</server.id>
				<webserver.host>localhost</webserver.host>
				<webserver.port>9090</webserver.port>
