<?xml version="1.0" encoding="UTF-8"?>
<or-mappings xmlns="http://osthus.de/ambeth/ambeth_orm_2_0">

	<link-mappings>
		<!-- cascade-delete="none | left | right | both", default "none" -->
		<link source="LINK_GROUP_GROUP" cascade-delete="none" />
	</link-mappings>

	<entity-mappings>

		<entity class="de.osthus.ambeth.persistence.xml.model.Group">
			<attributes>
				<relation name="ChildGroups" link="LINK_GROUP_GROUP" this="left"/>
				<relation name="ParentGroups" link="LINK_GROUP_GROUP" this="right"/> 
			</attributes>
		</entity>
	</entity-mappings>

</or-mappings>
