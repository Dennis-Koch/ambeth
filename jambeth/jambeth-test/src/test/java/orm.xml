<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://schema.kochdev.com/ambeth/ambeth_orm_1_2">

	<entity class="com.koch.ambeth.model.MaterialGroup" />

	<entity class="com.koch.ambeth.model.Material">
		<attributes>
			<basic name="CreatedOn">
				<column name="CREATED_ON" />
			</basic>
			<basic name="CreatedBy">
				<column name="CREATED_BY" />
			</basic>
			<basic name="UpdatedOn">
				<column name="UPDATED_ON" />
			</basic>
			<basic name="UpdatedBy">
				<column name="UPDATED_BY" />
			</basic>
		</attributes>
	</entity>

	<entity class="com.koch.ambeth.model.Unit">
		<table name="UNIT" />
		<attributes>
			<basic name="Name">
				<column name="NAME" />
			</basic>
		</attributes>
	</entity>

</entity-mappings>
