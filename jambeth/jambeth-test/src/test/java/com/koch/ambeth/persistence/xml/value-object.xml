	<entity class="com.koch.ambeth.persistence.xml.model.Employee">
		<value-object class="com.koch.ambeth.persistence.xml.model.EmployeeType">
			<basic name="CarMake" name-in-entity="Car.Make" />
			<basic name="CarModel" name-in-entity="Car.Model" />
		</value-object>
		<value-object class="com.koch.ambeth.persistence.xml.model.EmployeeSmallType">
			<basic name="CarMake" name-in-entity="Car.Make" />
			<basic name="CarModel" name-in-entity="Car.Model" />
		</value-object>
	</entity>

	<entity class="com.koch.ambeth.persistence.xml.model.Address">
		<value-object class="com.koch.ambeth.persistence.xml.model.AddressType" />
	</entity>

	<entity class="com.koch.ambeth.persistence.xml.model.Project">
		<value-object class="com.koch.ambeth.persistence.xml.model.ProjectType">
			<ignore name="IgnoreMe" />
		</value-object>
	</entity>

</entity-mappings>
