<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://www.osthus.de/ambeth/ambeth_orm_1_2">

	<entity class="de.osthus.ambeth.cache.cacheretriever.LocalEntity">
		<attributes>
			<to-one name="Parent" target-entity="de.osthus.ambeth.cache.cacheretriever.ExternalEntity2">
				<join-table name="LOCAL_ENTITY">
					<join-column name="PARENT" />
					<inverse-join-attribute name="Name" />
				</join-table>
			</to-one>
			<to-many name="Externals" target-entity="de.osthus.ambeth.cache.cacheretriever.ExternalEntity">
				<join-table name="LINK_LOC_ENTITY_EXT_ENTITY">
					<join-column name="LEFT_ID" />
					<inverse-join-column name="RIGHT_ID" />
					<inverse-join-attribute name="Name" />
				</join-table>
			</to-many>
			<to-one name="Sibling" target-entity="de.osthus.ambeth.cache.cacheretriever.ExternalEntity">
				<join-table name="LINK_LOCAL_TO_SIBLING">
					<join-column name="LEFT_ID" />
					<inverse-join-column name="RIGHT_ID" />
					<inverse-join-attribute name="Name" />
				</join-table>
			</to-one>
			<!-- <to-many name="externals2" target-entity="de.osthus.ambeth.cache.cacheretriever.ExternalEntity2">
				<join-table name="LOCAL_ENTITY">
					<join-column name="NAME" />
					<inverse-join-attribute name="local" />
				</join-table>
			</to-many> -->
		</attributes>
	</entity>

</entity-mappings>
