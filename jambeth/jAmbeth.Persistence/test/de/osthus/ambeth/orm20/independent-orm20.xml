<?xml version="1.0" encoding="UTF-8"?>
<or-mappings xmlns="http://osthus.de/ambeth/ambeth_orm_2_0">

	<link-mappings>
		<independent-link alias="Employee-Boat" cascade-delete="right" right="de.osthus.ambeth.orm20.Boat" />
		<independent-link alias="Employee-Address" cascade-delete="right" right="de.osthus.ambeth.orm20.Address" />
		<independent-link alias="Address-Employee" cascade-delete="left" left="de.osthus.ambeth.orm20.Address" />
		<independent-link alias="ParentA-ChildA" cascade-delete="right" right="de.osthus.ambeth.orm20.ChildA" />
	</link-mappings>

	<entity-mappings>

		<entity class="de.osthus.ambeth.orm20.Employee">
			<attributes>
				<id name="Id" />
				<alternate-id name="Name" />
				<version name="Version" />
				<created-by name="CreatedBy" />
				<created-on name="CreatedOn" />
				<updated-by name="UpdatedBy" />
				<updated-on name="UpdatedOn" />
				<basic name="Car.Make" column="CAR_MAKE" />
				<basic name="Car.Model" column="CAR_MODEL" />
				<relation name="Supervisor" link="Employee-Employee" this="left" />
				<relation name="Boat" link="Employee-Boat" />
				<relation name="PrimaryAddress" link="Employee-Address" />
				<relation name="OtherAddresses" link="Address-Employee" />
			</attributes>
		</entity>

		<entity class="de.osthus.ambeth.orm20.Address">
			<attributes>
				<id name="Id" />
				<version name="Version" />
				<created-by name="CreatedBy" />
				<created-on name="CreatedOn" />
				<updated-by name="CpdatedBy" />
				<updated-on name="CpdatedOn" />
				<relation name="Resident" link="Employee-Address" />
			</attributes>
		</entity>

		<entity class="de.osthus.ambeth.orm20.Boat">
			<attributes>
				<id name="Id" />
				<alternate-id name="Name" />
				<version name="Version" />
				<created-by name="CreatedBy" />
				<created-on name="CreatedOn" />
				<updated-by name="UpdatedBy" />
				<updated-on name="UpdatedOn" />
			</attributes>
		</entity>

		<entity class="de.osthus.ambeth.orm20.Project">
			<attributes>
				<id name="Id" />
				<version name="Version" />
				<created-by name="CreatedBy" />
				<created-on name="CreatedOn" />
				<updated-by name="UpdatedBy" />
				<updated-on name="UpdatedOn" />
				<ignore name="ArchivedOn" />
			</attributes>
		</entity>

		<external-entity class="de.osthus.ambeth.orm20.ParentA">
			<attributes>
				<id name="Id" />
				<version name="Version" />
				<created-by name="CreatedBy" />
				<created-on name="CreatedOn" />
				<updated-by name="UpdatedBy" />
				<updated-on name="UpdatedOn" />
				<relation name="Child" link="ParentA-ChildA" />
			</attributes>
		</external-entity>

		<external-entity class="de.osthus.ambeth.orm20.ChildA">
			<attributes>
				<id name="Id" />
				<version name="Version" />
				<created-by name="CreatedBy" />
				<created-on name="CreatedOn" />
				<updated-by name="UpdatedBy" />
				<updated-on name="UpdatedOn" />
			</attributes>
		</external-entity>

	</entity-mappings>

</or-mappings>
