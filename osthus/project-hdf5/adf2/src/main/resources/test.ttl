<http://purl.allotrope.org/member/ACME/test/data/schema/sample-record>
	a adf:Record;
	adf:field [
		adf:index "1"^^xs:integer;
		adf:name "sample_number"; 
		rdfs:label "sample number";
		adf:type adf:String;
		], [
		adf:index "2"^^xs:uri;
		adf:name "href"; #unique within record 
		rdfs:label "barcode";
		adf:type adf:String;
		];
	.
	
		
		

<http://purl.allotrope.org/member/ACME/test/data/schema/sample-set>
	a adf:DataSetSchema;
	adf:rank "1"^^xs:integer;
	adf:type <http://purl.allotrope.org/member/ACME/test/data/schema/sample-record>;
	adf:dimension [
		adf:index="1"^^xs:integer; # optional for rank < 2
		adf:unbounded "true"^^xs:boolean;
		adf:scale adf:DefaultScale; # scale = LinearScale [0:1:*]
	]
	.
			

	
<http://purl.allotrope.org/member/ACME/test/data/schema/lc-ms-set> 
		a				adf:DataSetSchema;
		adf:rank		"4"^^xs:integer;
		adf:type		xs:double;
		adf:dimension	[ 
				adf:describes ms:1000001; # sample number
				adf:index "1"^^xs:integer;
				rdfs:label "sample";
				adf:unbounded "true"^^xs:boolean; 
				adf:scale [
					a adf:MappedScale
					adf:scaleMap <http://purl.allotrope.org/member/ACME/test/sampleSet#field(1)>; # (slice) reference into 1d data set referencing record data type's  first field
					adf:endValue;
					adf:increment;
				]
			], [
				adf:describes ms:1000894; # retention time
				adf:index "2"^^xs:integer;
				rdfs:label "retention time";
				adf:unbounded "true"^^xs:boolean; 
				adf:scale [
					a adf:LinearScale
					adf:startValue "0.0";
					adf:increment "0.1";
					adf:referenceTime adf:InjectionTime;
					ms:has_units	uo:0000031; # min
				]
			],
			 [
				adf:index "3"^^xs:integer;
				rdfs:label "m/z";
				adf:scale [
					a adf:LinearScale;
					adf:startValue "0.0";
					adf:incrementValue  "1.0";
					ms:has_units	ms:UO1000040;
				]
			], [
				adf:index "4"^^xs:integer;
				rdfs:label "ic";
				adf:scale [
					a adf:LogarithmicScale; # example only
					adf:startValue "0.0";
					adf:incrementValue "0.1";
					ms:has_units	uo:UO0000189; # count
				]
			],
		.

<http://purl.allotrope.org/member/ACME/test/data/schema/chromatogram-set> 
		a				adf:DataSetSchema;
		adf:rank		"3"^^xs:integer;
		adf:dimension	[ 
				adf:describes ms:1000001; # sample number
				adf:index "1"^^xs:integer;
				rdfs:label "sample"; 
			], [
				adf:index "3"^^xs:integer;
				rdfs:label "ic";
				ms:has_units	uo:UO0000189; # count
			], [
				adf:describes ms:1000894; # retention time
				adf:index "4"^^xs:integer;
				rdfs:label "retention time";
				ms:has_units	uo:0000031; # min
			];
	.
<http://purl.allotrope.org/member/ACME/test/data/schema/mass-spectrum-set> 
		a				adf:DataSetSchema;
