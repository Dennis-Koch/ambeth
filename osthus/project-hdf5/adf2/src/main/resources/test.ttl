adf:SampleFeature a owl:Class ;
	rdfs:subClassOf cmo:0002746; # Experimental Sample
	rdfs:subClassOf ssn:FeatureOfInterest .

adf:ObserverableProperty a owl:Class;
	rdfs:subClassOf ssn:Property;
	rdfs:subClassOf qudt:Quantity;
	.
	

adf:Measurement a owl:Class ;
	rdfs:subClassOf ssn:Observation .
	
adf:measurementValue a owl:Property ;
	rdfs:domain adf:Measurement ;	
	rdfs:range adf:MeasurementValue .
	
adf:MeasurementValue a owl:Class ;
	rdfs:subClassOf ssn:ObservationValue ;
	rdfs:subClassOf qudt:QuantityValue .
		
